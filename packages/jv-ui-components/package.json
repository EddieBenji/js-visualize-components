{
  "name": "@jaspersoft/jv-ui-components",
  "version": "1.0.0",
  "private": true,
  "license": "MIT",
  "type": "module",
  "main": "dist/material-ui/index.umd.js",
  "module": "dist/material-ui/index.es.js",
  "types": "dist/material-ui/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "sass:jvui": "sass --no-source-map --style=expanded scss/jv-ui.scss ${BUILD_THEMES_DIR}/jv-ui.css",
    "styles": "dotenv -c -- run-p sass:jvui",
    "start": "onchange -i \"scss/**/*.*\" -- run-s styles",
    "start:dev": "onchange -i \"scss/**/*.*\" -- run-s styles copy:styles:dev",
    "copy:styles": "node scripts/copy-styles-context.js",
    "copy:styles:dev": "node scripts/copy-styles-context.js dev",
    "dev": "vite",
    "build": "dotenv -c -- run-s copy:styles styles vite:build",
    "vite:build": "tsc -b && vite build",
    "serve": "vite preview",
    "clean": "rm -rf dist",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jaspersoft/js-visualize-plugins.git",
    "directory": "packages/jv-ui-components"
  },
