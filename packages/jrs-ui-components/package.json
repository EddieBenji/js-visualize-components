{
  "name": "jrs-ui-components",
  "version": "0.0.1",
  "private": true,
  "license": "MIT",
  "main": "dist/material-ui/index.js",
  "files": [
    "dist"
  ],
  "type": "module",
  "types": "dist/material-ui/index.d.ts",
  "scripts": {
    "sass:jasperui": "sass --no-source-map --style=expanded scss/jasper-ui.scss ${BUILD_THEMES_DIR}/jasper-ui.css",
    "styles": "dotenv -c -- run-p sass:jasperui",
    "start": "onchange -i \"scss/**/*.*\" -- run-s styles",
    "start:dev": "onchange -i \"scss/**/*.*\" -- run-s styles copy:styles:dev",
    "copy:styles": "node scripts/copy-styles-context.js",
    "copy:styles:dev": "node scripts/copy-styles-context.js dev",
    "create:declarations": "tsc",
    "build": "dotenv -c -- run-s copy:styles styles create:declarations",
    "build-prod": "dotenv -c -- run-s copy:styles styles create:declarations",
    "clean": "rm -rf dist",
    "test": "NODE_OPTIONS='$NODE_OPTIONS --experimental-vm-modules' npx jest"
  },
  "dependencies": {
    "@emotion/is-prop-valid": "1.2.2",
    "@emotion/react": "11.4.1",
    "@emotion/sheet": "1.2.2",
    "@emotion/styled": "11.11.5",
    "@emotion/utils": "1.2.1",
    "@icons/material": "0.2.4",
    "@mui/base": "5.0.0-beta.38",
    "@mui/icons-material": "5.15.12",
    "@mui/lab": "5.0.0-alpha.167",
    "@mui/material": "5.15.12",
    "@mui/private-theming": "5.15.12",
    "@mui/styled-engine": "5.15.11",
    "@mui/system": "5.15.12",
    "@mui/utils": "5.15.12",
    "css-vendor": "2.0.8",
    "dom-helpers": "5.2.0",
    "focus-visible": "5.2.0",
    "hyphenate-style-name": "1.0.4",
    "jquery": "3.5.1",
    "jquery-ui": "1.13.2",
    "lodash": "4.17.21",
    "popper.js": "1.16.1-lts",
    "re-resizable": "6.9.0",
    "react": "^18.3.1",
    "react-color": "2.19.3",
    "react-dom": "^18.3.1",
    "react-is": "16.13.1",
    "react-transition-group": "4.4.5",
    "reactcss": "1.2.3",
    "sass": "^1.77.4",
    "scheduler": "^0.23.0",
    "underscore": "1.13.1",
    "underscore.string": "3.3.5"
  },
  "devDependencies": {
    "@babel/core": "^7.24.6",
    "@babel/preset-env": "^7.24.6",
    "@babel/preset-react": "^7.24.6",
    "@babel/preset-typescript": "^7.24.6",
    "@testing-library/jest-dom": "^6.4.5",
    "@testing-library/react": "^16.0.0",
    "@types/jest": "^29.5.12",
    "@types/jquery": "^3.5.5",
    "@types/material-ui": "^0.21.17",
    "@types/react": "^18.3.1",
    "@types/react-color": "3.0.10",
    "@types/react-dom": "18.2.19",
    "@types/underscore": "^1.10.24",
    "@typescript-eslint/eslint-plugin": "^4.15.0",
    "@typescript-eslint/parser": "^4.15.0",
    "dotenv": "^8.2.0",
    "dotenv-cli": "^4.0.0",
    "dotenv-expand": "^5.1.0",
    "glob": "^7.1.6",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "npm-run-all": "^4.1.5",
    "onchange": "^7.1.0",
    "typescript": "^5.4.5"
  }
}
